<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= article.body %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= article.published_at %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= article.author_id %>
  </p>

</div>
